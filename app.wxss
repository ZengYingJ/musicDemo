page {
    display: block;
    background: -webkit-linear-gradient(left top, black 50%, rgba(0, 0, 0, 0.4));
    background-attachment: fixed;
}

view {
    box-sizing: border-box;
    width: 100%;
    margin: 0px;
    padding: 0px;
}

.search {
    /* background:-webkit-linear-gradient(left ,gray , black,transparent);   */
    color: white;
    display: flex;
    font-size: 35rpx;
    padding: 30rpx;
    justify-content: space-between;
}

.search > navigator:first-of-type {
    display: flex;
    align-items: center;
}

.navigator {
    display: flex;
    justify-content: space-around;
    background: rgba(250, 250, 250, 0.4);
    font-size: 35rpx;
}

.navigator text {
    color: white;
    padding: 20rpx 10rpx;
}

.navigator .active {
    color: red;
    font-weight: 600;
    border-bottom: 2px solid red;
}

/*公共底部  */

footer {
    display: block;
    text-align: center;
    margin: 100rpx 0 20rpx 0;
    font-size: 23rpx;
    color: white;
}

