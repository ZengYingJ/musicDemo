/* 播放控件 */

.playSong {
    width: 100%;
    height: 150rpx;
    position: fixed;
    bottom: 0px;
    z-index: 10;
    transform: all 1s ease;
    display: flex;
    background: rgba(0, 0, 0, 0.8);
    color: white;
}

.animate {
    animation: animate 15s linear infinite;
    transform: rotate(0deg);
}

.singerImg {
    flex: 2;
    display: flex;
    justify-content: center;
}

.singerImg image {
    align-self: center;
    border-radius: 50%;
    width: 90%;
    height: 90%;
}

@keyframes animate {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

.songInfo {
    flex: 4;
    display: flex;
    padding-left: 20rpx;
    font-size: 30rpx;
    flex-direction: column;
    justify-content: center;
}

.songInfo .authorName {
    color: gray;
    font-size: 25rpx;
}

.playSong .btn {
    flex: 4;
    display: flex;
    font-size: 33rpx;
    align-self: center;
    justify-content: space-around;
}

.playSong .btn text {
    align-self: center;
}
