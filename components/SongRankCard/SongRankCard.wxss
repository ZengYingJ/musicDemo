/* components/SongRankCard/SongRankCard.wxss */
.container{
    position: relative;
    display: flex;
    align-items: center;
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx;
}
.container > .rank_image{
    position: relative;
    width: 230rpx;
    height: 230rpx;
    background-color: gray;
}
.container > .title{
    position: relative;
    margin-left: 50rpx;
    font-size: 40rpx;
    color: #ffffff;
}